<p id="notice"><%= notice %></p>

<h1>Testers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @testers.each do |tester| %>
      <tr>
        <td><%= tester.name %></td>
        <td><%= tester.body %></td>
        <td><%= link_to 'Show', tester %></td>
        <td><%= link_to 'Edit', edit_tester_path(tester) %></td>
        <td><%= link_to 'Destroy', tester, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tester', new_tester_path %>
